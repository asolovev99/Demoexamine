<Window x:Class="Demoexamine.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demoexamine"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="500">
    <Grid>
        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Width="50" Height="50">
            <Ellipse Fill="Yellow" Height="50" Width="50" Stroke="Black" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" >
            </Ellipse>
            <TextBlock Text="1" FontFamily="/Demoexamine;component/Document fonts/#Akrobat Bold" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" FontSize="18" TextAlignment="Center" Margin="0,14,0,0"/>
        </Grid>
    </Grid>

</Window>
